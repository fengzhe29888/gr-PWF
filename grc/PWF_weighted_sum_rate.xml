<?xml version="1.0"?>
<block>
  <name>Weighted Sum-Rate</name>
  <key>PWF_weighted_sum_rate</key>
  <category>PWF</category>
  <import>import PWF</import>
  <make>PWF.weighted_sum_rate($nlinks, $nt, $H, $ichn_gain_dB, $weights)</make>

  <param>
    <name>Number of Links (L)</name>
    <key>nlinks</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>TX Antenna Number</name>
    <key>nt</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>Channel (H)</name>
    <key>H</key>
    <type>raw</type>
  </param>
  <param>
    <name>interference gain (dB)</name>
    <key>ichn_gain_dB</key>
    <value>10</value>
    <type>int</type>
  </param>
  <param>
    <name>Weights</name>
    <key>weights</key>
    <type>raw</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$nt*$nt</vlen>
    <nports>$nlinks</nports>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
